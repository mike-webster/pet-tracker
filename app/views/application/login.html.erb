<h2>login</h2>
<div>
<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %>
</div>
<div>
  <%= form_for @user, url: "/login", method: "post" do |form| %>
    <%= form.label :email %>
    <%= form.text_field :email %>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <%= form.submit %>
  <% end %>
</div>